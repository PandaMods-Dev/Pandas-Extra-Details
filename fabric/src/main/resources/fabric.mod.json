{
  "schemaVersion": 1,
	"id": "${id}",
	"version": "${version}",
	"name": "${name}",
	"description": "${description}",
	"authors": [
		"The Panda Oliver"
	],
	"icon": "assets/extra_details/icon.png",
  "contact": {
		"issues": "https://github.com/PandaDap2006/Pandas-Extra-Details/issues",
		"sources": "https://github.com/PandaDap2006/Pandas-Extra-Details"
  },
  "license": "GPL-3",
  "environment": "client",
  "entrypoints": {
    "main": [
			"me.pandamods.extra_details.fabric.ExtraDetailsFabric"
    ],
		"modmenu": [
			"me.pandamods.extra_details.fabric.ModMenuIntegration"
		]
  },
  "mixins": [
		"extra_details.mixins.json",
    "extra_details-common.mixins.json"
  ],
  "depends": {
    "fabric": "*",
		"fabric-api": "*",
		"minecraft": "${compatible_fabricmc_versions}",
		"java": ">=${java_version}",
		"architectury": "*",
		"pandalib": ">=0.3"
  },
	"custom": {
		"modmenu": {
			"links": {
				"modmenu.discord": "https://discord.gg/wjPt4vEfXb",
				"modmenu.kofi": "https://ko-fi.com/pandamods",
				"modmenu.curseforge": "https://www.curseforge.com/minecraft/mc-mods/pandas-extra-details",
				"modmenu.modrinth": "https://modrinth.com/mod/pandas-extra-details",
				"modmenu.github_releases": "https://github.com/PandaDap2006/pandas-extra-details/releases"
			},
			"update_checker": true
		}
	}
}